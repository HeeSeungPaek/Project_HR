<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.olive.attendance.dao.CalendarDao">
	 <insert id="insert" parameterType="com.olive.dto.Calendar">	 
	 	INSERT INTO Calendar(STARTDATE, ENDDATE, CONTEXT) 
	 	VALUES(#{startdate}, #{enddate}, #{context})
	 </insert>
	 
	 <select id="selectlist" parameterType="com.olive.dto.Calendar">
	 SELECT * FROM Calendar  = #{startdate}, #{enddate}, #{context}

	 
	 
	 </select>
	 
</mapper>